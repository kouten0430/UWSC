KBD(VK_ESC,CLICK)
KBD(VK_ALT,UP)

SENDSTR(0,"ダミー")

REPEAT
	KBD(VK_SHIFT,DOWN,10)
	KBD(VK_F10,CLICK,10)
	KBD(VK_SHIFT,UP,10)

	KBD(VK_A,CLICK,10)

	SLEEP(0.5)

UNTIL POS(".pyw",GETSTR(0))>0

EXEC("cmd.exe")

